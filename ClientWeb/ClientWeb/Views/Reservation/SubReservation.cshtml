@model ClientWeb.Models.Reservation.SubReservationModel

@{
    ViewData["Title"] = "SubReservation";
}
<h1>SubReservation Form</h1>

<form asp-action="SubReservation" method="post">
    <div class="form-group">
        <label>Title :</label>
        <input asp-for="Title" class="form-control" required />
    </div>
    <div class="form-group">
        <label>Description :</label>
        <input asp-for="Description" class="form-control" required />
    </div>

    

    <button type="submit" class="btn btn-primary">Create SubReservation</button>
</form>
